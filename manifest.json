{
    "manifest_version": 3,
    "name": "Linkedin Voice Assistant",
    "version": "1.0",
    "description": "Browse Linkedin through voice",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}